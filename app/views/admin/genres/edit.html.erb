<h2 style= "margin-bottom: 20px;">ジャンル編集画面</h2>
<%= form_for @genre, url: admin_genre_path(@genre), method: :patch do |f| %>
    <%= f.label "ジャンル" %>
    <label><%= f.text_field :type %></label>
    <label><%= f.radio_button :sell_activated, true %>有効</label>
    <label><%= f.radio_button :sell_activated, false %>無効</label>
    <%= f.submit "変更を保存",class:'btn btn-primary' %>
<% end %>

